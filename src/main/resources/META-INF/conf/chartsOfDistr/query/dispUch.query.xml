<?xml version='1.0' encoding='UTF-8'?>

<query xmlns="http://n2oapp.net/framework/config/schema/query-5.0">
    <list result-mapping="['_embedded.disp']" count-mapping="['page.totalElements']">
        <!--        <rest>/data-api/patients/search/findAllPatientsFromDistr?distr={districtId}&amp;page={page}&amp;size={limit}</rest>-->

        <rest>/data-api/disp/search/findAllDispUchPatients?distr={districtId}&amp;page=0&amp;size=10</rest>
    </list>

    <fields>
        <field id="name" domain="string" mapping="['diagGroup']"/>
        <field id="value" domain="integer" mapping="['countPats']"/>
        <field id="distr_id" domain="string" mapping="['distr_id']"/>
    </fields>
    <filters>
        <like field-id="distr_id"
              filter-id="districtId"
        />
    </filters>
</query>
