<?xml version='1.0' encoding='UTF-8'?>

<query xmlns="http://n2oapp.net/framework/config/schema/query-5.0">
    <list result-mapping="['_embedded.countPatients']" count-mapping="['page.totalElements']">
        <rest>/data-api/countPatients/search/countPatientsByGroupsFromDistr?distr={districtId}&amp;page=0&amp;size=10</rest>
    </list>

    <fields>
<!--        <field id="id" domain="long" mapping="['id']"/>-->
        <field id="gender" domain="integer" mapping="['gender']"/>
        <field id="patCount" domain="integer" mapping="['patCount']"/>
    </fields>
    <filters>
        <like field-id="distr_id"
              filter-id="districtId"
        />
    </filters>
</query>
